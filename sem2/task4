#include <iostream>
#include <cmath>
using namespace std;

int main() {
    double ay, h;

    cout << "Enter vertical acceleration ay:\n";
    cin >> ay;

    cout << "Enter height h:\n";
    cin >> h;

    if (ay <= 0 || h <= 0) {
        cout << "Input error\n";
        return 0;
    }

    double t = sqrt(2 * h / ay);

    cout << "Time t = " << t << "\n";
    return 0;
}

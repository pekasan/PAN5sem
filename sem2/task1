#include <iostream>
using namespace std;

int main() {
    double S, V, rho, CL;

    cout << "Enter S:\n";
    cin >> S;

    cout << "Enter V:\n";
    cin >> V;

    cout << "Enter rho:\n";
    cin >> rho;

    cout << "Enter CL:\n";
    cin >> CL;

    double L = 0.5 * rho * V * V * S * CL;

    cout << "L = " << L << "\n";
    return 0;
}
